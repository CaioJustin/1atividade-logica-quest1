Var
A : inteiro
B : Inteiro
C : inteiro
Inicio

escreval ("Digite o valor de A:")
Leia(A)

escreval ("Digite o valor de B:")
Leia (B)


se (A = B) entao
  C <- A + B
  senao
  C <- A * B
  Fimse
  
escreval("O Valor de C e :",C)
  
//Mostra pro usuario
Limpatela
Escreval("  ----Valor das Letras----   ")
Escreval(" Digite o Valor da letra A:",A)
Escreval(" Digite o valor da letra B:",B)
Escreval("")
se (A = B) entao
  C <- A + B
  senao
  C <- A * B
  Fimse
Escreval(" Digite o valor da letra C:",C)




